{
    "name": "gocuber/framework",
    "description": "The Cuber Framework.",
    "keywords": ["framework", "cuber"],
    "license": "MIT",
    "homepage": "https://www.gocuber.com",
    "support": {
        "issues": "https://github.com/gocuber/framework/issues",
        "source": "https://github.com/gocuber/framework"
    },
    "authors": [
        {
            "name": "Cuber",
            "email": "cuber@gocuber.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "illuminate/container": "^5.7",
        "erusev/parsedown": "~1.7",
        "elasticsearch/elasticsearch": "~6.0",
        "phpmailer/phpmailer": "~6.0"
    },
    "autoload": {
        "files": [
            "src/Cuber/Support/helpers.php"
        ],
        "psr-4": {
            "Cuber\\": "src/Cuber/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
